{
  "name": "drei-kr/espocrm-api",
  "description": "PHP API for EspoCRM",
  "license": "MIT",
  "keywords": [
    "espocrm",
    "crm rest api",
	"espocrm api",
	"php crm api",
	"pluscrm",
	"opensource crm api"
  ],
  "homepage": "https://github.com/drei-kr/espocrm-api",
  "type": "library",
  "authors": [
    {
      "name": "Dominik Danninger",
      "email": "ddanninger@gmail.com",
      "homepage": "http://drei.kr"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/drei-kr/espocrm-api"
    }
  ],
  "autoload": {
    "psr-4": {
        "Drei\\EspoCRM\\": "src/",
        "Drei\\EspoCRM\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "~5",
    "guzzlehttp/guzzle-services": "^0.4"
  },
  "require-dev": {
	"phpunit/phpunit": "^5"
  },
  "extra": {
    "branch-alias": {
		"dev-master": "1.0-dev"
    }
  }
}
